<p>Delete Course</p>
<div class="container">
    @{ Html.BeginForm();}
    <div class="row">
        <div class="col-lg-3">
            @Html.Label("Course Name")
        </div>
        <div class="col-lg-3">
            @Html.TextBox("Name",(string)ViewBag.Name)
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            @Html.Label("Fees")
        </div>
        <div class="col-lg-3">
            @Html.TextBox("Fees",(float)ViewBag.Fees)
        </div>
    </div>
    <input type="submit" value="Delete" />
</div>